import random
number=random.randint(1,100)
for i in range(0,3):
    user=int(input("guess the number"))
    if user==number:
        print("Hurray!!")
        print("you guessed the number right it's{number}")
    break
if user>number:
    print("you guessed number is too high than required number")
else:
    print("you guessed number is too low than required number")
